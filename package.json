{
  "name": "@eibii/tail-uix",
  "private": false,
  "version": "0.0.8",
  "description": "Biblioteca de componentes Vue com Tailwindcss",
  "author": "ERC Dev <dev@eibii.com> (https://www.ercdev.com.br/)",
  "homepage": "https://tailuix.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eibii/tail-uix"
  },
  "keywords": [
    "tailwindcss",
    "vue",
    "vue-tailwind",
    "vue components",
    "tailwind components"
  ],
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/style.css"
  },
  "browser": {
    "./styles.css": "./dist/style.css"
  },
  "module": "./dist/tail-uix.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && npx tailwindcss -o src/assets/css/build.css --minify && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "maska": "^2.1.11",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vitepress": "^1.0.0-rc.44",
    "vue-tsc": "^1.8.27"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
